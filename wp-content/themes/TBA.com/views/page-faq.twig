{% extends "base.twig" %}

{% block content %}
	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
				<h1>{{post.title}}</h1>
				<div class="article-body">
					{{post.content}}
										
					{% if post.get_field('faq') %}
					
					{% for faq in post.get_field('faq') %}
					<h4>{{residential.title}}</h4>
					<h5>{{faq.question}}</h5>
		            <p>{{faq.answer}}</p>
		            {% endfor %}
					{% endif %}	
					
					<p>{{ post.get_field('instant_quote_button') }}</p>

		
						
				</div>		
			</section>
		</article>
	</div><!-- /content-wrapper -->
{% endblock %}